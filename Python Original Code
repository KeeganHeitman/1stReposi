__import__(pygame)

pygame.time.delay.(XXXXXXXXXXXXXXXXXXXX)

clock = pygame.time.Clock()
flag=true
while flag:
    clock.tick(NUMBER OF ANIMATION IMAGES)
    
events = pygame.event.get()
for events in events:
    if event.type == pygame.KEYDOWN:
    if event.key == pygame.K_LEFT
    location -= 1
    if event.key == pygame.K_RIGHT
    location += 1
   
move_ticker = 0
keys=pygame.key.get_pressed()
if keys[K_LEFT]:
    if move_ticker == 0:
    move_ticker = 10
    location -= 1
    if location == -1
        location = 0
if keys[K_RIGHT]:
    move_ticker == 0:
    move_ticker = 10
    location+=1
    if location == 5:
        location ==4

if move_ticker > 0:
    move_ticker-= 1

location-=1
iflocation==-1:
    location=0
    
location -= 1
location = max(800,600)
